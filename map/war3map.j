function Trig_Special4Agree_Actions takes nothing returns nothing
	call DisableTrigger(GetTriggeringTrigger())
	call EnableTrigger(gg_trg_SpecialXDialog)
	call DisableTrigger(gg_trg_SpecialXStart)
